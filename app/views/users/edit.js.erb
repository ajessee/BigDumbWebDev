if (window.utils.modal.userEditModal) {
  window.utils.modal.userEditModal.innerHTML = "";
  window.utils.modal.userEditModal.insertAdjacentHTML('beforeend', "<%= j (render 'edit') %>");
} else {
  if (window.innerWidth> 1440) {
    window.utils.modal.userEditModal = window.utils.modal.openModal('block', '0%', 3, 5, 12, 8, null, null, false);
  } else {
    window.utils.modal.userEditModal = window.utils.modal.openModal('block', '0%', 2, 3, 14, 12, null, null, false);
  }
  // Setup modal window, insert new user signup partial
  window.utils.modal.userEditModal.insertAdjacentHTML('beforeend', "<%= j (render 'edit') %>");
}
// Get close button and attach close modal function
window.utils.modal.editUserCloseButton = document.querySelector("#edit-user-close-button");
window.utils.modal.editUserCloseButton.addEventListener('click', function(e) {
  e.preventDefault();
  window.utils.modal.closeModal(e);
  window.utils.modal.userEditModal = null;
})
// Get debug information
<% if Rails.env.development? %>
window.utils.debug.debugPanel.innerHTML = "";
window.utils.debug.debugPanel.insertAdjacentHTML('beforeend', `<%= debug(params) %>`);
<% end %>
// Initialize labels component
window.utils.labels.init();

